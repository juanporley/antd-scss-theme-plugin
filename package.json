		"babel-loader": "^8.2.2",
		"babel-plugin-add-module-exports": "^1.0.4",
		"cross-env": "^7.0.3",
		"eslint": "^7.18.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^7.2.0",
		"eslint-import-resolver-node": "^0.3.4",
