	},
	"keywords": [
		"webpack",
		"plugin",
		"live-reload",
		"ant-design",
		"less",
		"sass",
		"scss",
		"less-loader",
		"sass-loader",
		"styles"
	],
	"author": "Juan Porley <juan@regalvoice.com>",
	"license": "BSD-2-Clause",
	"bugs": {
		"url": "https://github.com/juanporley/antd-scss-theme-plugin/issues"
	},
	"homepage": "https://github.com/juanporley/antd-scss-theme-plugin",
	"files": [
		"build"
	],
	"dependencies": {
		"less": "^4.1.0",
		"less-vars-to-js": "^1.3.0",
		"loader-utils": "^2.0.0"
